{
  "name": "AI语音合成",
  "short_name": "语音合成",
  "start_url": "./",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#4CAF50",
  "icons": [
    {
      "src": "icons/icon-192.svg",
      "sizes": "192x192",
      "type": "image/svg+xml"
    },
    {
      "src": "icons/icon-512.svg",
      "sizes": "512x512",
      "type": "image/svg+xml"
    }
  ]
}
{
    "manifest_version": 3,
    "name": "GPT-SoVITS",
    "version": "1.0",
    "description": "用于合成语音",
    "icons": [
        {
            "src": "icon_iapp.png",
            "sizes": "225x225"
        }
    ],
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["activeTab"]
}

